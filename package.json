{
  "name": "skill-sharing",
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "build": "npm run test && npm run check",
    "start": "node src/main.js",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js --verbose",
    "check": "prettier . --check && eslint src test",
    "format": "prettier . --write"
  },
  "dependencies": {
    "express": "4.18.2"
  },
  "devDependencies": {
    "@types/express": "4.17.17",
    "@types/jest": "29.5.5",
    "eslint": "8.49.0",
    "jest": "29.7.0",
    "prettier": "3.0.3",
    "supertest": "6.3.3"
  }
}
