{
  "name": "skill-sharing",
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "build": "npm run test && npm run check",
    "start": "node src/server/main.js",
    "devserver": "nodemon src/server/main.js",
    "test": "node --no-warnings=ExperimentalWarning --experimental-vm-modules node_modules/jest/bin/jest.js",
    "check": "prettier . --check && eslint public src tests",
    "check2": "tsc && prettier . --check && eslint public src tests",
    "format": "prettier . --write && eslint --fix public src tests"
  },
  "dependencies": {
    "express": "4.18.2",
    "lit-html": "2.8.0"
  },
  "devDependencies": {
    "@types/express": "4.17.17",
    "@types/jest": "29.5.5",
    "@types/supertest": "2.0.12",
    "eslint": "8.49.0",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "nodemon": "3.0.1",
    "prettier": "3.0.3",
    "supertest": "6.3.3",
    "typescript": "5.2.2"
  }
}
