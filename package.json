{
  "name": "skill-sharing",
  "version": "0.1.0",
  "author": "Falko Schumann",
  "license": "MIT",
  "engines": {
    "node": ">=18.7.0"
  },
  "type": "module",
  "main": "src/main.js",
  "scripts": {
    "start": "node src/main.js",
    "test": "NODE_OPTIONS=\"--experimental-global-customevent --experimental-vm-modules\" npx jest",
    "build": "rollup -c"
  },
  "dependencies": {
    "@muspellheim/shared": "falkoschumann/muspellheim-shared-javascript",
    "express": "4.21.0",
    "lit-html": "3.2.0"
  },
  "devDependencies": {
    "@jest/globals": "29.7.0",
    "@rollup/plugin-commonjs": "28.0.0",
    "@rollup/plugin-json": "6.1.0",
    "@rollup/plugin-node-resolve": "15.3.0",
    "@types/express": "5.0.0",
    "@types/supertest": "6.0.2",
    "eslint": "9.12.0",
    "eventsource": "2.0.2",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "nodemon": "3.1.7",
    "prettier": "3.3.3",
    "puppeteer": "23.4.1",
    "rollup": "4.22.5",
    "supertest": "7.0.0"
  },
  "bin": "build/index.js",
  "pkg": {
    "assets": "public/**/*",
    "targets": [
      "node18-linux-x64",
      "node18-macos-x64",
      "node18-win-x64"
    ],
    "outputPath": "dist"
  }
}
