  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "28.0.1",
    "@rollup/plugin-json": "6.1.0",
    "@rollup/plugin-node-resolve": "15.3.0",
    "@types/express": "5.0.0",
    "@types/supertest": "6.0.2",
    "@vitest/coverage-v8": "2.1.4",
    "eslint": "9.14.0",
    "eventsource": "2.0.2",
    "jsdom": "25.0.1",
    "nodemon": "3.1.7",
    "prettier": "3.3.3",
    "puppeteer": "23.7.0",
    "rollup": "4.24.4",
    "supertest": "7.0.0",
    "vitest": "2.1.4"
